{
  "name": "Stega",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Mask your emails",
  "permissions": [
    "https://en.wikipedia.org/"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://mail.google.com/*"
      ],
      "js": [
        "js/jquery.min.js",
        "js/markov.min.js",
        "js/main.js"
      ],
      "run_at": "document_end",
      "all_frames": false
    }
  ]
}
